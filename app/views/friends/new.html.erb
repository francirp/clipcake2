<h1>New Friend</h1>


<%= form_tag(friends_url, method: 'post') do %>


  <div>
    <%= label_tag :friend.id %><br />
    <%= number_field_tag :friend.id %>
  </div>

    <div>
    <%= label_tag :user_id %><br />
    <%= number_field_tag :user_id %>
  </div>

    <div>
    <%= label_tag :uid %><br />
    <%= number_field_tag :uid %>
  </div>

  
  <div>
    <%= submit_tag "Create Friend" %>
  </div>
<% end %>
